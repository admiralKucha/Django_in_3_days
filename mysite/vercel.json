{
  "builds": [
    {
        "src": "mysite/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "mysite/wsgi.py"
        },

      {
      "src": "(.*)",
      "dest": "$1"
      }
    ]
}
